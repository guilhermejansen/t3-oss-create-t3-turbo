{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "module": "ES2022",
    "noEmit": false,
    // "emitDeclarationOnly": true,
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "baseUrl": ".",
